    <title> Vaccine Tracker </title>

    <!-- Css Stylesheets -->
    <link rel="stylesheet" href="~/Asset2/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Asset2/css/simple-line-icons.css">
    <link rel="stylesheet" href="~/Asset2/css/all.min.css">
